# Sample inventory file for Ubuntu patching collection
# Copy this file and modify for your environment

[k8s_cluster]
k8s-master ansible_host=192.168.1.10
k8s-worker1 ansible_host=192.168.1.11
k8s-worker2 ansible_host=192.168.1.12
k8s-worker3 ansible_host=192.168.1.13

[dockerhost]
dockerhost ansible_host=192.168.1.20

[all:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=~/.ssh/id_rsa
ansible_become=true
ansible_python_interpreter=/usr/bin/python3

[k8s_cluster:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=~/.ssh/k8s_key
ansible_become=true
k8_primary_node=k8-node-01

[docker_hosts:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=~/.ssh/docker_key
ansible_become=true